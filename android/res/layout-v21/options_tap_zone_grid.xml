<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/base_dialog" 
    android:orientation="vertical"
    android:minHeight="1600dip"
    android:minWidth="1600dip"
    android:layout_marginLeft="3dip"
    android:layout_marginRight="3dip"
    android:layout_marginTop="3dip"
    android:layout_marginBottom="3dip"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">
    <TableLayout
            android:id="@+id/tl_buttons"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dip"
            android:orientation="vertical">
        >
        <TableRow>
            <ImageButton android:id="@+id/btn_plus_left"
                    android:layout_width="0dip"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:background="?attr/colorThemeGray2"
                    android:tag="tint"
                    android:layout_marginTop="2dip"
                    android:layout_marginBottom="2dip"
                    android:layout_marginLeft="2dip"
                    android:layout_marginRight="2dip"
                    android:src="?attr/attr_icons8_minus_small"
                    android:layout_gravity="center_vertical"
                    />
            <TextView android:id="@+id/txt_perc_left"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal|center_vertical"
                    android:gravity="center_horizontal"
                    android:textColor="?attr/colorIcon"
                    android:text="%"
                    android:layout_margin="3dp"
                    />
            <ImageButton android:id="@+id/btn_minus_left"
                    style="@style/TextAppearance.Small"
                    android:layout_width="0dip"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:src="?attr/attr_icons8_plus_small"
                    android:tag="tint"
                    android:background="?attr/colorThemeGray2"
                    android:layout_marginTop="2dip"
                    android:layout_marginBottom="2dip"
                    android:layout_marginLeft="2dip"
                    android:layout_marginRight="2dip"
                    android:layout_gravity="center_vertical"
                    />
            <TextView
                    android:layout_width="0dip"
                    android:layout_weight="3"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal|center_vertical"
                    android:gravity="center_horizontal"
                    android:textColor="?attr/colorIcon"
                    android:text="@string/non_sensitive"
                    android:layout_margin="3dp"
                    />
            <ImageButton android:id="@+id/btn_minus_right"
                    style="@style/TextAppearance.Small"
                    android:layout_width="0dip"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:src="?attr/attr_icons8_plus_small"
                    android:tag="tint"
                    android:background="?attr/colorThemeGray2"
                    android:layout_marginTop="2dip"
                    android:layout_marginBottom="2dip"
                    android:layout_marginLeft="2dip"
                    android:layout_marginRight="2dip"
                    android:layout_gravity="center_vertical"
